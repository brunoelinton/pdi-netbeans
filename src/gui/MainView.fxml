<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainViewController">
   <children>
      <MenuBar prefWidth="800.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arquivo">
            <items>
              <MenuItem fx:id="menuItemFechar" mnemonicParsing="false" onAction="#onMenuItemFechar" text="Fechar" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Processamento">
            <items>
              <MenuItem fx:id="menuItemVonKriesMedia" mnemonicParsing="false" onAction="#onMenuItemProcessamentoLote" text="Processamento em Lote" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ajuda">
            <items>
              <MenuItem fx:id="menuItemSobre" mnemonicParsing="false" onAction="#onMenuItemSobre" text="Sobre" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <StackPane fx:id="parentContainer" VBox.vgrow="ALWAYS">
         <children>
            <AnchorPane fx:id="anchorRoot" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" StackPane.alignment="CENTER" />
         </children>
      </StackPane>
   </children>
</VBox>
